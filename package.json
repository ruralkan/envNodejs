{
  "name": "envnodejs",
  "version": "1.0.0",
  "description": "Environment to develop js app",
  "main": "app.js",
  "scripts": {
    "prestart": "babel-node utils/startMessage.js",
    "dev": "nodemon --exec npm run start",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node buildScripts/app.js",
    "lint": "esw buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruralkan/envNodejs.git"
  },
  "keywords": [
    "Node.js",
    "environment",
    "dev",
    "boilerplate"
  ],
  "author": "Manuel Mejia Perales",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ruralkan/envNodejs/issues"
  },
  "homepage": "https://github.com/ruralkan/envNodejs#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-watch": "^5.1.2",
    "localtunnel": "^1.9.2",
    "nodemon": "^1.19.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5"
  }
}
